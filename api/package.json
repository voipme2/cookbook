{
  "name": "cookbook-api",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node dist/app.js",
    "dev": "ts-node-dev --respawn --transpile-only src/app.ts",
    "build": "tsc",
    "test": "mocha \"spec/**/*.{js,mjs}\"",
    "test:ci": "npm run build && mocha \"spec/**/*.{js,mjs}\"",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "import-recipes": "node scripts/import-recipes.js",
    "migrate-production": "node scripts/migrate-production.js"
  },
  "dependencies": {
    "@types/cors": "^2.8.19",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "express": "^5.2.0",
    "jsdom": "^26.1.0",
    "multer": "^2.0.1",
    "parse-duration": "^0.1.1",
    "pg": "^8.16.3",
    "prettier": "^3.6.2",
    "slugify": "^1.6.3",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/jsdom": "^21.1.6",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.0.10",
    "@types/pg": "^8.11.10",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "chai": "^5.2.1",
    "eslint": "^9.30.1",
    "jasmine": "^5.8.0",
    "mocha": "^11.7.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5"
  }
}
